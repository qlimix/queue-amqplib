{
    "name": "qlimix/queue-amqplib",
    "type": "library",
    "description": "Queue implemented with amqplib",
    "license": "MIT",
    "keywords": [
        "queue",
        "amqp",
        "amqplib"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "php-amqplib/php-amqplib": "^2.7",
        "qlimix/queue": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Qlimix\\Tests\\Queue\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    }
}
